<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.Dao.ItripCommentMapper">
    <insert id="addComment" >
        insert into itrip_comment values(null,#{hotelId},#{productId},#{orderId},#{productType},#{content},#{userId},#{isHavingImg},#{positionScore}
        ,#{facilitiesScore},#{serviceScore},#{hygieneScore},#{score},#{tripMode},#{isOk}
        ,#{creationDate},#{createdBy},#{modifyDate},#{modifiedBy})
    </insert>
</mapper>